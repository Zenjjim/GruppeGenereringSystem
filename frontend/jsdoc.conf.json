{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "source": {
    "include": [
      "src"
    ],
    "includePattern": ".+\\.ts(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/better-docs/category",
    "node_modules/better-docs/typescript"
  ],
  "opts": {
    "destination": "docs",
    "recurse": true,
    "readme": "README.md",
    "template": "node_modules/better-docs"
  }
}